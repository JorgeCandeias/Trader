{
  "ConnectionStrings": {
    "Trader": "<ConnectionString>"
  },

  "Binance": {
    "ApiKey": "<ApiKey>",
    "SecretKey": "<SecretKey>",
    "BaseApiAddress": "https://api.binance.com",
    "BaseWssAddress": "wss://stream.binance.com:9443",
    "UsageWarningRatio": 0.80,
    "UsageBackoffRatio": 0.95,
    "Timeout": "1.00:00:00.000"
  },

  "Trader": {
    "BatchTickDelay": "0.00:00:05.000",
    "PingDelay": "0.00:00:01.000",
    "BatchEnabled": true,
    "SwapPool": {
      "AutoAddEnabled": false,
      "AutoRedeemSavings": false,
      "Assets": [ "BTC", "ETH", "BUSD" ]
    },
    "Algos": {
      "Oscillator-BNBUP": {
        "Type": "Oscillator",
        "Symbols": [ "BNBUPUSDT" ],
        "KlineInterval": "Hours1",
        "KlinePeriods": 100,
        "Enabled": false,
        "Options": {
          "Oversold": 10,
          "TakeProfitRate": 0.20,
          "Notional": 20,
          "UseProfits": true
        }
      },
      "Discovery1": {
        "Type": "Discovery",
        "Symbols": [ "BTCBUSD", "BNBBUSD" ],
        "Enabled": true,
        "BatchOrder": 1000,
        "Options": {
          "QuoteAssets": [ "BTC" ],
          "IgnoreSymbols": [ "ETHBTC", "BNBBTC", "XMRBTC" ],
          "ReportSavings": true
        }
      },
      "Portfolio": {
        "Type": "Portfolio",
        "MaxExecutionTime": "1.00:00:00.000",
        "Enabled": true,
        "Symbols": [
          "ADABTC"
        ],
        "KlineInterval": "Hours1",
        "KlinePeriods": 100,
        "BatchOrder": 900,
        "Options": {
          "UseSavings": true,
          "UseSwapPools": true,
          "EntryBuy": {
            "Enabled": true
          },
          "SellOff": {
            "Enabled": false,
            "ExcludeSymbols": [ "BNBBTC", "XMRBTC" ]
          },
          "TopUpBuy": {
            "Enabled": false,
            "Sma": {
              "Enabled": false
            },
            "Rsi": {
              "Enabled": false
            }
          },
          "Recovery": {
            "Enabled": true
          }
        }
      }
    }
  },

  "ValueAveragingTemplate1": {
    "Symbols": [
      "COMPBTC"
    ],
    "KlineInterval": "Hours1",
    "KlinePeriods": 100,
    "Enabled": false,
    "Options": {
      "RedeemSavings": true,
      "RedeemSwapPool": true,
      "CooldownPeriod": "1.00:00:00.000",
      "MaxPositions": 10,
      "BuyQuoteBalanceFraction": 0.001
